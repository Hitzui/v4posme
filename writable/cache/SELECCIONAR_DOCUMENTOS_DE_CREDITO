					}
					else
					{
						var data		= [];
						var objListaFilasSeleccionadas 	= $('tr.row-selected');
						for(var i = 0 ; i < objListaFilasSeleccionadas.length ; i++){
							var ipush 	= {};
							ipush 		= objTableListView.fnGetData(objListaFilasSeleccionadas[i]);
